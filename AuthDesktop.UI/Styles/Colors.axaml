<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  
  <SolidColorBrush x:Key="ListItemNormalBrush" Color="#b6cde0"/>
  <SolidColorBrush x:Key="ListItemHoverBrush" Color="#9abedb"/>
  <SolidColorBrush x:Key="ListItemSelectedBrush" Color="#78b0de"/>
  
  <Color x:Key="StartBorderGradientColor">#EAEFEF</Color>
  <Color x:Key="EndBorderGradientColor">#BFC9D1</Color>
  
  <Color x:Key="DangerColor">#9c0303</Color>
  <Color x:Key="WarningColor">#F57C00</Color>
  <Color x:Key="SuccessColor">#215c2e</Color>
  <Color x:Key="InformationColor">#151f61</Color>

  </ResourceDictionary>